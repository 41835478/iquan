.cate-section{style: "display:block"}
  .coupon-list
    #couponList2
      - if coupons
        - coupons.each do |coupon|
          = render partial: "m_diyquan/tb_shuangpai_item", locals: {coupon: coupon}
      - if zhekous
        - zhekous.each do |zhekou|
          = render partial: "m_diyquan/tb_zhekou_shuangpai_item", locals: {coupon: zhekou}

  %p.txt-loading
  .download-fixed
    %a{href: "/lanlan_download/", target: "_blank"}
      %i
  .search-fixed
    %a{href: "/query/"}
      %i
